def _GetDefines(config):
    'Returns the list of preprocessor definitions for this configuation.\n\n  Arguments:\n    config: The dictionary that defines the special processing to be done\n            for this configuration.\n  Returns:\n    The list of preprocessor definitions.\n  '
    defines = []
    for d in config.get('defines', []):
        if (type(d) == list):
            fd = '='.join([str(dpart) for dpart in d])
        else:
            fd = str(d)
        defines.append(fd)
    return defines
