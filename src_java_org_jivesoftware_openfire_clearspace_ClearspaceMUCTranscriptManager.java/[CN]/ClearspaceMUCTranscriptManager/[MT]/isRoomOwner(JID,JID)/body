{
  if (user == null || roomJID == null) {
    return false;
  }
  MultiUserChatService chatService=XMPPServer.getInstance().getMultiUserChatManager().getMultiUserChatService(roomJID);
  MUCRoom room=chatService.getChatRoom(roomJID.getNode());
  return room != null && room.getOwners().contains(user);
}
