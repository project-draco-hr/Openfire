{
  if (executor != null) {
    executor.shutdown();
  }
  for (  Plugin plugin : plugins.values()) {
    try {
      plugin.destroyPlugin();
    }
 catch (    Exception e) {
      Log.error(e.getMessage(),e);
    }
  }
  plugins.clear();
  pluginDirs.clear();
  pluginFiles.clear();
  classloaders.clear();
  pluginDevelopment.clear();
  childPluginMap.clear();
  pluginMonitor=null;
}
