def GetBundleContentsFolderPath(self):
    "Returns the qualified path to the bundle's contents folder. E.g.\n    Chromium.app/Contents or Foo.bundle/Versions/A. Only valid for bundles."
    if self.isIOS:
        return self.GetWrapperName()
    assert self._IsBundle()
    if (self.spec['type'] == 'shared_library'):
        return os.path.join(self.GetWrapperName(), 'Versions', self.GetFrameworkVersion())
    else:
        return os.path.join(self.GetWrapperName(), 'Contents')
