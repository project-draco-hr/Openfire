def _GetDebugInfoPostbuilds(self, configname, output, output_binary, quiet):
    "Returns a list of shell commands that contain the shell commands\n    neccessary to massage this target's debug information. These should be run\n    as postbuilds before the actual postbuilds run."
    self.configname = configname
    result = []
    if (self._Test('GCC_GENERATE_DEBUGGING_SYMBOLS', 'YES', default='YES') and self._Test('DEBUG_INFORMATION_FORMAT', 'dwarf-with-dsym', default='dwarf') and (self.spec['type'] != 'static_library')):
        if (not quiet):
            result.append(('echo DSYMUTIL\\(%s\\)' % self.spec['target_name']))
        result.append(('dsymutil %s -o %s' % (output_binary, (output + '.dSYM'))))
    self.configname = None
    return result
